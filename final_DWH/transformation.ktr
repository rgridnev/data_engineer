    <connection>flights</connection>
    <sql>select
	t.passenger_id,
	f.actual_departure,
	f.actual_arrival,
	-- тут делаем именно f.actual_departure - f.actual_departure, а не просто хардкодим 0 чтобы отличить невылетевших от незадержавшихся
	coalesce (f.actual_departure - f.scheduled_departure, f.actual_departure - f.actual_departure) as departure_delay,
	f.aircraft_code as aircraft_id,
	f.departure_airport as departure_airport_id,
	f.arrival_airport as arrival_airport_id,
	tf.fare_conditions,
	tf.amount
from
	tickets t 
	left join ticket_flights tf on t.ticket_no = tf.ticket_no 
	left join flights f on tf.flight_id = f.flight_id;</sql>
